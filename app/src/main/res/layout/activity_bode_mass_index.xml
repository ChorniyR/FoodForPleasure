            app:layout_constraintEnd_toEndOf="parent"
            android:textStyle="bold"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            android:backgroundTint="#80000000" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.498"
