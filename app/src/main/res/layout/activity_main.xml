<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <ImageView
            android:src="@drawable/mainmenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView2"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0"/>

    <ImageView
            android:layout_width="381dp"
            android:layout_height="288dp" app:srcCompat="@android:drawable/dark_header"
            android:id="@+id/imageView_DarcBackGround"
            android:background="#80000000" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.134" app:layout_constraintHorizontal_bias="0.47"/>
    <TextView
            android:text="Food For Pleasure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/TextBox_ApplicationName"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.022"
            android:textStyle="bold" app:layout_constraintHorizontal_bias="0.497"/>
    <TextView
            android:text="Белки"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/TextView_Proteins"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.317"
            android:textStyle="bold" app:layout_constraintHorizontal_bias="0.087"/>
    <TextView
            android:text="Жиры"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/TextView_Fats"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.317"
            android:textStyle="bold" app:layout_constraintHorizontal_bias="0.912"/>
    <TextView
            android:text="Углеводы"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/TextView_Сarbohydrates"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.317"
            android:textStyle="bold"/>
    <TextView
            android:text="0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/UserProteins"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.412"
            android:textStyle="bold" app:layout_constraintHorizontal_bias="0.143"/>
    <TextView
            android:text="0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/UserFats"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.412"
            android:textStyle="bold" app:layout_constraintHorizontal_bias="0.861"/>
    <TextView
            android:text="0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/UserCarbohydrates"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.412"
            android:textStyle="bold"/>
    <TextView
            android:text="Из"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/TextView_From"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.217"
            android:textStyle="bold"/>
    <TextView
            android:text="0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/DailyCalories"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.217"
            android:textStyle="bold" app:layout_constraintHorizontal_bias="0.726"/>
    <TextView
            android:text="Килокалорий"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/TextView_Calories"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.107"
            android:textStyle="bold"/>
    <TextView
            android:text="0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/UserCalories"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:textColor="@android:color/white" android:textSize="24sp" android:fontFamily="sans-serif-thin"
            app:layout_constraintVertical_bias="0.217"
            android:textStyle="bold" app:layout_constraintHorizontal_bias="0.271"/>

    <Button
            android:text="Settings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/settingsButton"
            android:onClick="onSettingsButtonClick"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.875"
            app:layout_constraintVertical_bias="0.898"/>
</androidx.constraintlayout.widget.ConstraintLayout>